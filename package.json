{
  "name": "@figma/my-make-file",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "vercel-build": "vite build",
    "dev": "vite",
    "test:web": "playwright test",
    "test:web:ui": "playwright test --ui",
    "test:web:debug": "playwright test --debug",
    "test:api": "newman run tests/api/collection.json",
    "test:perf": "k6 run tests/perf/load-test.js",
    "lighthouse": "lighthouse --config-path=./configs/lighthouse.config.js --output html --output-path ./reports/lighthouse/report.html",
    "report": "playwright show-report",
    "allure:generate": "allure generate allure-results --clean -o allure-report",
    "allure:open": "allure open allure-report",
    "allure:clean": "rm -rf allure-results allure-report",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "security:audit": "npm audit --production",
    "security:check": "snyk test",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
